{
  "name": "lup-expires",
  "version": "1.0.0",
  "description": "Offers different data structures that allow temporary storing of objects",
  "files": [
    "lib/**/*"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LupCode/node-lup-expires.git"
  },
  "keywords": [
    "data-structures",
    "expire",
    "ExpireMap",
    "ExpireSet"
  ],
  "author": "LupCode.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LupCode/node-lup-expires/issues"
  },
  "homepage": "https://github.com/LupCode/node-lup-expires#readme",
  "dependencies": {
    
  },
  "devDependencies": {
    "@types/jest": "^27.5.0",
    "@types/node": "^17.0.31",
    "jest": "^28.0.3",
    "prettier": "^2.6.2",
    "ts-jest": "^28.0.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.6.4"
  }
}
